{
  "name": "n8n-nodes-aircall",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Aircall cloud phone system providing 10 resources and 50+ operations for call management, user administration, contacts, SMS messaging, and workflow automation.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-aircall.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-aircall",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-aircall/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "aircall",
    "phone",
    "telephony",
    "voip",
    "call-center",
    "cloud-phone",
    "sms",
    "voice",
    "crm",
    "automation",
    "workflow"
  ],
  "main": "dist/nodes/Aircall/Aircall.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AircallApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Aircall/Aircall.node.js",
      "dist/nodes/Aircall/AircallTrigger.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "copyfiles -u 1 \"nodes/**/*.svg\" \"nodes/**/*.png\" dist/",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
